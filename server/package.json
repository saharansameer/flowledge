    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "express": "^5.1.0",
    "inngest": "^3.40.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  }
